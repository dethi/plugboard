APP_NAME='{{app.app_name}}'
APP_ENV=production
APP_KEY='{{app.app_key}}'
APP_DEBUG=false
APP_LOG=daily
APP_LOG_LEVEL=info
APP_URL='https://{{app.app_domain[0]}}'

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE='{{app.app_db.name}}'
DB_USERNAME='{{app.app_db.user}}'
DB_PASSWORD='{{app.app_db.password}}'

CACHE_DRIVER=redis
CACHE_PREFIX={{app.app_id}}

QUEUE_DRIVER=redis

SESSION_DRIVER=cookie
SESSION_COOKIE_NAME='{{app.app_id}}_session'
SESSION_SECURE_COOKIE=true

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DATABASE={{app.app_redis}}

MAIL_DRIVER=mailgun
MAILGUN_DOMAIN='{{app.app_mailgun.domain}}'
MAILGUN_SECRET='{{app.app_mailgun.secret}}'
